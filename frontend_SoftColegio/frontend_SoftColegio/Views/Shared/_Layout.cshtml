    <script>

        function NotificacionInfoDerecha(mensajes) {
            $.NotificationApp.send("Aviso!", mensajes, "top-right", "rgba(0,0,0,0.2)", "info")
        }

        function NotificacionSuccessDerecha(mensaje) {
            $.NotificationApp.send("DATOS CORRECTOS!", mensaje, "top-right", "rgba(0,0,0,0.2)", "success")
        }

        function NotificacionAvisoDerecha(mensaje) {
            $.NotificationApp.send("AVISO!", mensaje, "top-right", "rgba(0,0,0,0.2)", "warning")
        }

        function fn_ArmadoAJAX(pstrMetodo, pParam, successFn, errorFn) {
            var oParametros = JSON.stringify(pParam);

            //Arma Parametros
            var parametro = '';
            if (oParametros === '') {
                parametro = "{}";
            } else {
                parametro = oParametros;
            }
            //Ejecuta Ajax
            $.ajax({
                type: "POST",
                url: pstrMetodo,
                contentType: "application/json; charset=utf-8",
                data: parametro,
                dataType: "json",
                async: true,
                success: function (data) {
                    successFn(data);
                },
                error: errorFn
            });

        }

        function fnDesconectar() {
            $.ajax({
                type: "POST",
                url: RutaPrincipal + 'Login/cerrarSesion',
                async: false,
                success: function (msg) {
                    window.location.href = RutaPrincipal + "Login/Index?ivalorsesion=0&valorlogin=vacio";

                },
                error: function (msg) {

                    //location.reload();
                },
                complete: function (data) {
                    // si acabo de completar
                }
            });
        }

